<script>
    clock();
    setInterval(function(){
        clock();
    },1000);
    function clock(){
        var d = new Date();
        var year = d.getFullYear();
        var month = d.getMonth()+1;
        var dt = d.getDate();
        var day = d.getDay();
        var hour = d.getHours()<10?'0'+d.getHours():d.getHours();
        var minute = d.getMinutes()<10?'0'+d.getMinutes():d.getMinutes();
        var hours = hour+':'+minute;
        var timer = document.getElementsByClassName('time-l')[0].innerHTML=hours;
        var days = new Array('星期日','星期一','星期二','星期三','星期四','星期五','星期六');
        var times = year+'年'+month+'月'+dt+'日';
        var week = document.getElementsByClassName('time-top')[0].innerHTML=days[day];
        var under = document.getElementsByClassName('time-under')[0].innerHTML=times;
    };

    var fc = document.querySelectorAll('.fc-two');
    for(var i=0;i<fc.length;i++){
        fc[i].index = i;
        fc[i].onclick=function(){
            for(var j=0;j<fc.length;j++){
                fc[j].style.color = '#333';
                fc[j].style.fontWeight = 'normal';
            }
            fc[this.index].style.color="#cc0110";
            fc[this.index].style.fontWeight = 'bold';
        }
    }

    var zhi = document.querySelector('.zhi');
    var he = document.querySelector('.fc-he');
    zhi.onclick=function(){
        he.style.display='block';
    }

    var yi = document.querySelector('.yili');
    var fin = document.querySelector('.fc-yin');
    yi.onclick=function(){
        fin.style.display='block';
    }

    var hl = document.querySelector('.hli');
    var huo = document.querySelector('.huo');
    hl.onclick=function(){
        huo.style.display='block';
    }

    var big = document.querySelectorAll('.zp');
    var bt = document.querySelector('.datu img');
    var cuo = document.querySelector('.cuo');
    for(var k=0;k<big.length;k++){
        big[k].onclick=function(){
            bt.style.display='block';
            cuo.style.display='block';
        }
    }
    cuo.onclick=function(){
        bt.style.display='none';
        cuo.style.display='none';
    }
</script>
