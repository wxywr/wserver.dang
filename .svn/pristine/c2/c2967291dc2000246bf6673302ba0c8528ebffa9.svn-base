        <div class="read-s">
            <div class="container">
                <div v-for="child in data.children[0].children" class="eig" v-on:click="showBook(child.linkUrl)"><div class="slide"><img v-bind:src="child.contPic"/><p>{{child.contName}}</p></div></div>
