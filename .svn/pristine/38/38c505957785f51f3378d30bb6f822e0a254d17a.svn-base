    <script type="text/javascript" language="javascript">
        var Strview = "NetVideoX1";	//设置全局变量,控制控件的相对控件ID,默认值.
        var Strdiv = "view1";       //显示窗口的ID，默认值
        //摄像机登录
        function Login() {
            if(get_el(Strview)){
                get_el(Strview).m_szDeviceName = "";					//DVS名称,可以为空
                get_el(Strview).m_szUrl = '192.168.55.100';		//'DVS地址
                get_el(Strview).m_dwDataPort = 5000;		//'通讯端口
                get_el(Strview).m_szUserName = 'admin'; 	//用户名
                get_el(Strview).m_szPassword = 'admin'; 		//密码
                get_el(Strview).m_ChnNo = 0;	//同时打开DVS的所有通道：NetVideoX1.ChannelNum=0
//            get_el(Strview).SetLoginMode(1);	//设置登录方式
                get_el(Strview).Login();			//调用控件的登陆函数
            }
        }

        //启动预览
        function channelview() {
            if(get_el(Strview)){
                get_el(Strview).ChannelNum= 4  //打开第四通道，若同时打开DVS的所有通道：NetVideoX1.ChannelNum=0
                get_el(Strview).StartPreview();		//启动预览
            }
        }

        function start(){
            Login();
            channelview();
        }

        //选择播放显示窗口
        function reSelected(a, b) {
            if(get_el(Strview)){
                //alert(a + "  " + b);
                Strview = a;
                Strdiv = b;
                window.status = Strview;
                get_el(Strview).SetMouseEvent(0);
            }
        }

        //通过元素的ID特性来获取元素
        function get_el() {
            var elements = new Array();
            for (var i = 0; i < arguments.length; i++) {
                var element = arguments[i];
                if (typeof element == 'string') {
                    element = document.getElementById(element);
                }
                if (arguments.length == 1) {
                    return element;
                }
                elements.push(element);
            }
            return elements;
        }

        function takePicture(){
            if (jQuery(".under-a").css("background-position") == '155px -95px') {
                jQuery(".under-a").css("background-position", "155px 15px");
                jQuery('#view1').html("<object name=\"NetVideoX1\" ID=\"NetVideoX1\" width=\"100%\" height=\"100%\" CLASSID=\"CLSID:b125ad2d-a87e-417f-9012-992a16e164d0\" CODEBASE=\"/NetVideoX.exe#Version=2,0,0,0\"></object>");
                setTimeout(function(){start()},2000);
            } else {
                jQuery(".under-a").css("background-position", "155px -95px");
                var pic = get_el(Strview).ChannelCapture(1, 0);
                logout();
                jQuery.ajax({ //将照片转移到服务器
                    type:'post',
                    url:'movepic',
                    data:{'path':pic},
                    dataType:'json',
                    success:function(resp){
                        if(resp.success){
                            jQuery('#view1').html("<img id='myphoto' style='height:100%;' src='"+resp.path+"'/>")
                        }
                    }
                });
            }
        }

        //注销
        function logout(){
            if(get_el(Strview)){
                get_el(Strview).StopPreview();	//停止预览
                get_el(Strview).Logout();	    //退出登陆
            }
        }

        function showSmileWall(){
            getFaceImgs();
            if(jQuery('#NetVideoX1')){
                jQuery("#NetVideoX1").hide();
            }
            jQuery('.smile-wall').show();
            logout();
        }

        function closeWall(){
            jQuery('.smile-wall').hide();
            if(jQuery('#NetVideoX1')){
                jQuery("#NetVideoX1").show();
            }
            setTimeout(function(){start()},1000);
        }

        function getFaceImgs(){
            jQuery.ajax({
                type:'post',
                url:'getpicture',
                dataType:'json',
                success:function(resp){
                    if(resp.success){
                        var imgs = resp.items;
                        if(imgs.length>0){
                            jQuery(".faceImg").attr("src","tmpls/wserver.dang/images/holdplace.png");
                            var imgEl = jQuery(".faceImg");
                            imgEl[22].setAttribute("src",imgs[0].path);
                            for(var i=0;i<imgs.length;i++){
                                if(i==22){
                                    continue;
                                }
                                if(i<22){
                                    if(i + 1 < imgs.length){
                                        imgEl[i].setAttribute("src",imgs[i+1].path);
                                    }
                                }else{
                                    imgEl[i].setAttribute("src",imgs[i].path);
                                }

                            }
                        }
                    }
                }
            });
        }

        function showDao(){
            if(jQuery('#NetVideoX1')){
                jQuery("#NetVideoX1").hide();
            }
            jQuery('#view').show();
            logout();
        }

        function closeDao(){
            jQuery('#view').hide();
            if(jQuery('#NetVideoX1')){
                jQuery("#NetVideoX1").show();
            }
            setTimeout(function(){start()},1000);
        }

        function closePicture(){
            jQuery('.wall-da').hide();
        }

        function showDelete(){
            jQuery('.smile-wall .delete').show();
            var imgEl = jQuery(".faceImg");
            var delEl = jQuery(".delCrose");
            for(var i = 0;i<imgEl.length;i++){
                if(imgEl[i].getAttribute("src") != "tmpls/wserver.dang/images/holdplace.png"){
                    delEl[i].style.display = "block";
                    delEl[i].setAttribute("myIndex",i);
                }
            }

//            jQuery('.smile-wall .shan').show();
        }

        function closeShan(){
            jQuery('.smile-wall .delCrose').hide();
            jQuery('.smile-wall .delete').hide();
        }

        function showPhoto(me){
            if(me.getAttribute('src') != "tmpls/wserver.dang/images/holdplace.png"){
                jQuery('.wall-da').show();
                jQuery('.wall-da div img').attr('src',me.getAttribute('src'));
            }
        }

        //照片删除
        function delFace(me){
            if(window.confirm('确定删除吗？')){
                delet(me);
            }
        }

        function delet(me){
            var index = me.getAttribute('myIndex');
            var imgEl = jQuery(".faceImg");
            var facepath = imgEl[index].getAttribute("src");
            jQuery.ajax({
                type:'post',
                url:'deleteface',
                data:{'facepath':facepath},
                dataType:'json',
                success:function(resp){
                    if(resp.success){
                        me.style.display = "none";
                        imgEl[index].setAttribute("src","tmpls/wserver.dang/images/holdplace.png");
                    }
                }
            });
        }

        function printPicture(){
            if(jQuery("#myphoto").length>0){
                var imgpath = jQuery("#myphoto").attr('src');
                jQuery.ajax({
                    type:'post',
                    url:'print',
                    data:{'imgpath':imgpath},
                    dataType:'json',
                    success:function(resp){
                        if(resp.success){
                            alert("照片正在打印，请稍等...");
                        }else{
                            alert(resp.error);
                        }
                    }
                });
            }else{
                alert("请先拍照，再打印照片！");
            }
        }

        function compose(){
            if(jQuery("#myphoto").length<=0){
                alert("请先拍照，在合成照片！");
                return;
            }
            var dx = jQuery(".usethis").attr("dx");
            var dy = jQuery(".usethis").attr("dy");
            var bg = jQuery(".usethis").attr("bg");
            var img = jQuery("#myphoto").attr("src");
            jQuery.post({
                url:'compose',
                data:{'dx':dx,'dy':dy,'bg':bg,'img':img},
                dataType:'json',
                success:function(resp){
                    if(resp.success){
                        jQuery("#myphoto").attr("src",resp.path);
                        jQuery('.bg-top').children(".Photograph .p-right .bg .bg-top button").remove();
                    }else{
                        alert(resp.error);
                    }
                },
                error:function(){
                    alert("网络错误！");
                }
            });

        }
    </script>
</head>
